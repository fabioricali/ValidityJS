{
  "name": "bejs",
  "version": "1.0.0",
  "description": "Simple, light-weight assertions framework for javascript",
  "main": "index.js",
  "scripts": {
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "build": "webpack --progress --env.compress",
    "doc": "jsdox --output docs src/*",
    "test": "mocha test"
  },
  "keywords": [
    "is",
    "boolean",
    "url",
    "number",
    "string",
    "email",
    "object",
    "check",
    "float",
    "alphanumeric"
  ],
  "author": "Fabio Ricali",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "del": "^3.0.0",
    "jsdox": "^0.4.10",
    "mocha": "^3.4.2",
    "unminified-webpack-plugin": "^1.2.0",
    "webpack": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabioricali/beJS"
  }
}
